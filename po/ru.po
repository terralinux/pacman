msgid "MODIFIED\t%s\n"
msgstr "ИЗМЕНЁН\t%s\n"

#, c-format
msgid "Not Modified\t%s\n"
msgstr "Не изменён\t%s\n"

#, c-format
msgid "MISSING\t\t%s\n"
msgstr "ОТСУТСТВУЕТ\t\t%s\n"

