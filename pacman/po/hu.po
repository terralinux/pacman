msgid "      --logfile <path> set an alternate log file\n"
msgstr "      --logfile <útv.> alternatív logfájl beállítása\n"

#, c-format
msgid "      --noconfirm      do not ask for any confirmation\n"
msgstr "      --noconfirm      ne kérjen soha megerősítést\n"

